!!!TEST
scalar test = "INTEGER"
	@check(_ > 0)
	@check (_ < 1000)
	@check (_ != 5)
	@check "not_6_or_12" (_ != 6)
	@check "not_6_or_12" (_ != 12);
!!!RESULT
CREATE DOMAIN test AS INTEGER
	CONSTRAINT not_6_or_12 CHECK ((VALUE <> 6) AND (VALUE <> 12))
	CONSTRAINT test_check CHECK (((VALUE > 0) AND (VALUE < 1000)) AND (VALUE <> 5));
